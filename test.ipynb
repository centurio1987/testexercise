    "list = [8,4,5,6,7]\n",
    " \n",
    "for i in range(len(list)):\n",
    "    a = list[i]\n",
    "    for j in range(i, -1, -1):\n",
    "        if a < list[j]:\n",
    "            list[j+1] = list[j]\n",
    "            list[j] = a\n",
    "        else:\n",
    "            pass\n",
    "            \n",
